{
	"name": "aaronbullard/litmus",
    "description": "Color analysis API",
    "authors": [
        {
            "name": "Aaron Bullard",
            "email": "aaron.bullard77@gmail.com"
        }
    ],
	"keywords": ["color", "laravel", "litmus", "API"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.0.*",
		"way/generators": "1.0.*@dev",
		"crlosmify/modelo": "dev-master",
		"barryvdh/laravel-debugbar": "dev-master",
		"laravelbook/ardent": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/libraries/Helpers",
			"app/libraries/Litmus"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
