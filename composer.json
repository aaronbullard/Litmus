{
	"name": "aaronbullard/litmus",
    "description": "Color analysis API",
    "authors": [
        {
            "name": "Aaron Bullard",
            "email": "aaron.bullard77@gmail.com"
        }
    ],
	"keywords": ["color", "laravel", "litmus", "API"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.0.*",
		"way/generators": "1.0.*@dev",
		"crlosmify/modelo": "dev-master",
		"barryvdh/laravel-debugbar": "dev-master",
		"laravelbook/ardent": "2.4.1",
		"iron-io/iron_mq": "1.4.*"
	},
	"repositories": [
			{
					"type": "package",
					"package": {
							"name": "laravelbook/ardent",
							"version": "2.4.1",
							"source": {
									"type": "git",
									"url": "https://github.com/bexarcreativeinc/ardent.git",
									"reference": "master"
							},
							"require": {
									"php": ">=5.3.0",
									"illuminate/support": "4.x",
									"illuminate/database": "4.x",
									"illuminate/validation": "4.x"
							},
							"autoload": {
									"psr-0": {
											"LaravelBook\\Ardent": "src/"
									}
							}
					}
			}
	],
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/libraries/Helpers",
			"app/libraries/Litmus"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"php artisan migrate",
			"bower install"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"prefer-stable": true
}
